    "remap-istanbul": "^0.6.4",
    "ts-node": "^0.5.5",
    "tslint": "^3.13.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
